    "marked": "^15.0.6",
    "marked-highlight": "^2.2.1",
    "msw": "^2.10.2",
    "openapi-fetch": "^0.13.8",
    "openapi-msw": "^1.2.0",
    "pinia": "^2.3.0",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
